        <meta charset="UTF-8" />

        <!-- Set the viewport width to device width for mobile -->
        <meta name="viewport" content="width=device-width" />

        <meta http-equiv="cache-control" content="no-cache">
        <meta http-equiv="pragma" content="no-cache">

        <title>{% block title %}Welcome to LimTox{% endblock %}</title>

        {% stylesheets 
            'stylesheets/base.css' filter='cssrewrite' 
            'stylesheets/foundation.css' 
            'stylesheets/app.css' 
            'stylesheets/jquery-ui-1.8.21.custom.css' 
            'stylesheets/datepicker.css' 
            'stylesheets/pagerfantaDefault.css' 
            'stylesheets/tablesorter/themes/blue/style.css' 
            'stylesheets/stickytooltip.css'  
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}    
            
        {% javascripts filter='?uglifyjs2'
            '@FrontendBundle/Resources/public/js/foundation/modernizr.foundation.js'
            '@FrontendBundle/Resources/public/js/foundation/foundation.min.js'
            '@FrontendBundle/Resources/public/js/jquery-ui-1.9.js'
            '@FrontendBundle/Resources/public/js/foundation/app.js'
            '@FrontendBundle/Resources/public/js/foundation/foundation-datepicker.js'
            '@FrontendBundle/Resources/public/js/tablesorter/jquery-latest.js'
            '@FrontendBundle/Resources/public/js/stickytooltip.js'
        %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {# javascript applet for chemical generator #}
        
        {% block stylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>

    <body>
        <div id="wrapper">
            {% block body %}{% endblock %}
            <script src="{{ asset('javascripts/jquery.popupWindow.js') }}"></script>


            {% block javascripts_footer %}{% endblock %}
        </div>


