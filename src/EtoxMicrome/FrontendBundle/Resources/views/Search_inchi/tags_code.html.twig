    <dd class="active"><a href="#compounds" name="compoundDict"><span data-tooltip class="has-tip" title="Chemical compound search">Compounds</span></a></dd>
  {% else %}
    <dd><a href="#compounds" name="compoundDict"><span data-tooltip class="has-tip" title="Chemical compound search">Compounds</span></a></dd>
  {% endif %}

