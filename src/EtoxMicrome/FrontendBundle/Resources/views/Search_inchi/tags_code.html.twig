                    {% else %}
                        <span data-tooltip class="has-tip" title="Search in PubMed abstracts (chemicals indexed by MeSH)"><label for="radioSource"><input name="keywordSource" type="radio" id="radioKeywordSource" style="display:none;" value="abstract"><span class="custom radio"></span> Abstracts</label></span>
                    {% endif %}
                </fieldset>
            </div>
