                    {% endif %}
                </fieldset>
        </div>
        <div class="large-4 columns">
                <fieldset>
                    <legend>Source:</legend>
                    {% if source is defined %}
                        {% if source not in ["abstract","pubmed","fulltext","nda","epar"] %}
                            {% set keywordSource = default_source %}
                        {% else %}
                            {% set keywordSource = source %}
                        {% endif %}
                    {% else %}
                        {% set keywordSource = default_source %}
                    {% endif %}
                    {% if keywordSource=="all" %}
